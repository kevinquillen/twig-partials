{
  "name": "kevinquillen/twig-partials",
  "description": "Django-style partial template rendering for Twig.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "twig/twig": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
